  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:ru": "ng serve --aot --i18n-file=src/i18n/messages.ru.xlf --locale=ru --i18n-format=xlf",
    "build:ru": "ng build --aot --i18n-file=src/i18n/messages.ru.xlf --locale=ru --i18n-format=xlf",
